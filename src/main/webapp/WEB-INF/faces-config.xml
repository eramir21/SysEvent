<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    
    <faces-config-extension>
        <facelets-processing>
            <file-extension>.xhtml</file-extension>
            <process-as>xhtml</process-as>
        </facelets-processing>
    </faces-config-extension>
    
    <navigation-rule>            
        <navigation-case>
            <from-outcome>crear_cliente_form</from-outcome>
            <to-view-id>/administracion/cliente/Create.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>    
    <navigation-rule>            
        <navigation-case>
            <from-outcome>consulta_cliente_form</from-outcome>
            <to-view-id>/administracion/cliente/List.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    
     <navigation-rule>            
        <navigation-case>
            <from-outcome>crear_evento_form</from-outcome>
            <to-view-id>/administracion/evento/Create.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>    
    <navigation-rule>            
        <navigation-case>
            <from-outcome>consulta_evento_form</from-outcome>
            <to-view-id>/administracion/evento/List.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    
    
    <navigation-rule>            
        <navigation-case>
            <from-outcome>consulta_producto_form</from-outcome>
            <to-view-id>/administracion/producto/List.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    
    
       <navigation-rule>            
        <navigation-case>
            <from-outcome>consulta_proveedor_form</from-outcome>
            <to-view-id>/administracion/proveedor/List.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    
    
</faces-config>
